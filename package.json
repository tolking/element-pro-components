{
  "name": "element-pro-components",
  "version": "1.2.2",
  "description": "a component library for Vue 3 base on element-plus",
  "main": "lib/index.umd.js",
  "module": "lib/index.es.js",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    },
    "./lib/": "./lib/"
  },
  "types": "types/index.d.ts",
  "style": "lib/styles/index.css",
  "sideEffects": [
    "lib/styles/*"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run check && npm run build:lib && npm run build:helper && npm run build:styles && npm run build:types",
    "build:lib": "rimraf lib && vite build -c build/build-lib.ts",
    "build:types": "rimraf types && vue-tsc --declaration --emitDeclarationOnly",
    "build:helper": "esno build/build-helper.ts",
    "build:styles": "rimraf lib/styles && esno build/build-styles.ts",
    "build:docs": "npm run check && rimraf dist && npm run build:manifest && npm run build:ssr && esno build/prerender.ts",
    "build:manifest": "vite build --ssrManifest --base=/element-pro-components/ --outDir ../dist/static",
    "build:ssr": "vite build --ssr src/entry-server.ts --base=/element-pro-components/ --outDir ../dist/server",
    "check": "npm run lint:test && vue-tsc --noEmit",
    "test": "vitest",
    "lint": "eslint . --fix --ext .ts,.vue,.js",
    "lint:test": "eslint . --ext .ts,.vue,.js --max-warnings 0",
    "prettier": "prettier --check --write --ignore-unknown \"{build,demo,src,docs,test}/**\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tolking/element-pro-components.git"
  },
  "keywords": [
    "element-pro",
    "pro-components",
    "admin",
    "element-plus",
    "components",
    "vue",
    "ui"
  ],
  "author": "tolking <qw13131wang@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tolking/element-pro-components/issues"
  },
  "homepage": "https://github.com/tolking/element-pro-components#readme",
  "peerDependencies": {
    "@vueuse/core": "^9",
    "element-plus": "^2",
    "vue": "^3.2",
    "vue-router": "^4"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@vueuse/core": "^9.3.0",
    "element-plus": "^2.2.18",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@docsearch/css": "^3.2.1",
    "@docsearch/js": "^3.2.1",
    "@stackblitz/sdk": "^1.8.0",
    "@types/lodash": "^4.14.186",
    "@types/lodash-es": "^4.17.6",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/server-renderer": "^3.2.40",
    "@vue/test-utils": "^2.1.0",
    "@vueuse/head": "^0.9.8",
    "autoprefixer": "^10.4.12",
    "components-helper": "^2.1.3",
    "cssnano": "^5.1.13",
    "escape-html": "^1.0.3",
    "eslint": "^8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "esno": "^0.16.3",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-container": "^3.0.0",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "sitemap": "^7.1.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "vite-plugin-pwa": "^0.12.8",
    "vite-plugin-vue-markdown": "^0.22.1",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13",
    "workbox-window": "^6.5.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "markdown-it",
        "react-dom",
        "react"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "vetur": {
    "tags": "lib/tags.json",
    "attributes": "lib/attributes.json"
  },
  "web-types": "lib/web-types.json",
  "files": [
    "lib",
    "types"
  ]
}
